---
extends:
  - "defaults/configurations/walmart/es5-node"
rules:
  # Some overrides to match the existing style of `stylus-loader`.
  complexity: 0
  eqeqeq:
    - 2
    - allow-null
  func-style: 0
  max-params: 0
  max-statements: 0
  quotes:
    - 2
    - single
  space-before-function-paren:
    - 2
    - anonymous: never
      named: never
